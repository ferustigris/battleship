#:kivy 1.0.9

<OceanGame>:    
    fieldLeft: fieldGridLeft
    fieldRight: fieldGridRight
    GridLayout:
        id: fieldGridLeft
        pos: 0, 0
        size: root.width/2, root.height
        cols: 5
    GridLayout:
        id: fieldGridRight
        pos: root.center_x, 0
        size: root.width/2, root.height
        cols: 5
 
